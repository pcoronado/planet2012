<!DOCTYPE html> 
<html> 
<head> 
  <title>Planet</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>

  <script type="text/javascript">
    $(function() {
      $( "#trip_description" ).cleditor();
    });
  </script>

</head>
<body id="planet"> 
  <div id="banner"> 
    <%= image_tag("logo3.png") %>
    <%= @page_title || "Planet Travel Site" %>
  </div> 
  <div id="columns"> 
    <div id="side"> 
      <%= link_to "Home", planet_index_path %><br /> 
      <%= link_to "Tipos",  types_path %><br /> 
      <%= link_to "Sitios", sites_path %><br /> 
      <%= link_to "Contact", planet_contact_path %><br /> 
      <%= link_to 'Ejemplo',planet_ejemplo_path%><br /> 
      <%= link_to "Authors", planet_author_path %><br /> 
      <%= link_to "Ordenados", ordered_index_types_path %><br /> 
    </div> 
    <div id="main"> 
    <%= link_to 'Index',planet_index_path%>
    <%= link_to 'Contact',planet_contact_path%>
    <%= link_to 'Ejemplo',planet_ejemplo_path%>
    <%= link_to "Authors", planet_author_path %>
    <%= link_to "Tipos", types_path %>
    <%= link_to "Ordenados", ordered_index_types_path %>
      <%= yield %> 
      <br /><br />
    <%= link_to 'Index',planet_index_path%>
    <%= link_to 'Contact',planet_contact_path%>
    <%= link_to 'Ejemplo',planet_ejemplo_path%>
    <%= link_to "Authors", planet_author_path %>
    <%= link_to "Tipos", types_path %>
    <%= link_to "Ordenados", ordered_index_types_path %>
    </div> 
  </div> 
</body> 
</html> 